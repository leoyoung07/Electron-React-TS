{
  "name": "electron-react-ts",
  "version": "0.0.0",
  "license": "MIT",
  "author": "leoyoung",
  "main": "dist/main.js",
  "scripts": {
    "dev": "yarn compile-main-dev && concurrently \"yarn start-dev-server\" \"yarn start-electron\"",
    "compile-main-dev": "webpack --config config/webpack.config.main.dev.ts",
    "start-dev-server": "webpack-dev-server --inline --progress --config config/webpack.config.renderer.dev.ts",
    "start-electron": "electron --inspect=5858 dist/main.js",
    "compile": "webpack --config config/webpack.config.ts",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^3.2.1",
    "react-uwp": "^1.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.0.4",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "^3.0.0",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-merge": "^4.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^9.4.0",
    "electron-builder": "^20.41.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "build": {
    "appId": "com.leoyoung.electron-react-ts",
    "productName": "Electron-React-Ts",
    "copyright": "Copyright Â© year ${author}",
    "forceCodeSigning": false,
    "files": [
      "dist/**"
    ],
    "asarUnpack": [
    ],
    "extraResources": [],
    "extraFiles": [],
    "mac": {},
    "mas": {},
    "dmg": {},
    "pkg": {},
    "win": {
      "target": "portable"
    },
    "nsis": {},
    "nsisWeb": {},
    "portable": {},
    "appx": {},
    "squirrelWindows": {}
  }
}
